{
  "dashboard": {
    "id": null,
    "title": "HuskyApply 10x Scaling Dashboard",
    "tags": ["huskyapply", "scaling", "hpa"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Pod Scaling Overview",
        "type": "graph",
        "targets": [
          {
            "expr": "kube_deployment_status_replicas{deployment=~\"gateway|brain\",namespace=\"huskyapply\"}",
            "legendFormat": "{{deployment}} - Current Replicas"
          },
          {
            "expr": "kube_deployment_spec_replicas{deployment=~\"gateway|brain\",namespace=\"huskyapply\"}",
            "legendFormat": "{{deployment}} - Desired Replicas"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "max": 50
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "HPA Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "kube_hpa_status_current_replicas{hpa=~\"gateway-.*hpa|brain-.*hpa\",namespace=\"huskyapply\"}",
            "legendFormat": "{{hpa}} - Current"
          },
          {
            "expr": "kube_hpa_spec_min_replicas{hpa=~\"gateway-.*hpa|brain-.*hpa\",namespace=\"huskyapply\"}",
            "legendFormat": "{{hpa}} - Min"
          },
          {
            "expr": "kube_hpa_spec_max_replicas{hpa=~\"gateway-.*hpa|brain-.*hpa\",namespace=\"huskyapply\"}",
            "legendFormat": "{{hpa}} - Max"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "CPU Utilization",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{pod=~\"gateway-.*|brain-.*\",namespace=\"huskyapply\"}[5m]) * 100",
            "legendFormat": "{{pod}} - CPU %"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "max": 200,
            "unit": "percent"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Memory Utilization",
        "type": "graph",
        "targets": [
          {
            "expr": "container_memory_working_set_bytes{pod=~\"gateway-.*|brain-.*\",namespace=\"huskyapply\"} / container_spec_memory_limit_bytes{pod=~\"gateway-.*|brain-.*\",namespace=\"huskyapply\"} * 100",
            "legendFormat": "{{pod}} - Memory %"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Request Rate (Gateway)",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{service=\"gateway\",namespace=\"huskyapply\"}[5m])) by (method, status)",
            "legendFormat": "{{method}} {{status}}"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "unit": "reqps"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Response Latency P95",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"gateway\",namespace=\"huskyapply\"}[5m]))",
            "legendFormat": "Gateway P95 Latency"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "unit": "s"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Job Queue Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "huskyapply_jobs_pending_total{namespace=\"huskyapply\"}",
            "legendFormat": "Pending Jobs"
          },
          {
            "expr": "huskyapply_jobs_processing_total{namespace=\"huskyapply\"}",
            "legendFormat": "Processing Jobs"
          },
          {
            "expr": "rate(huskyapply_jobs_completed_total{namespace=\"huskyapply\"}[5m])",
            "legendFormat": "Completion Rate"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "AI Processing Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "huskyapply_ai_queue_depth{namespace=\"huskyapply\"}",
            "legendFormat": "AI Queue Depth"
          },
          {
            "expr": "histogram_quantile(0.90, rate(huskyapply_ai_processing_duration_seconds_bucket{namespace=\"huskyapply\"}[5m]))",
            "legendFormat": "AI Processing P90"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "RabbitMQ Queue Status",
        "type": "graph",
        "targets": [
          {
            "expr": "rabbitmq_queue_messages_ready{queue=\"job_processing\"}",
            "legendFormat": "Messages Ready"
          },
          {
            "expr": "rabbitmq_queue_messages_unacknowledged{queue=\"job_processing\"}",
            "legendFormat": "Messages Unacked"
          },
          {
            "expr": "rate(rabbitmq_queue_messages_published_total{queue=\"job_processing\"}[5m])",
            "legendFormat": "Publish Rate"
          },
          {
            "expr": "rate(rabbitmq_queue_messages_delivered_total{queue=\"job_processing\"}[5m])",
            "legendFormat": "Delivery Rate"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 10,
        "title": "Error Rates",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{service=\"gateway\",namespace=\"huskyapply\",status=~\"5.*\"}[5m])) / sum(rate(http_requests_total{service=\"gateway\",namespace=\"huskyapply\"}[5m])) * 100",
            "legendFormat": "Gateway Error Rate %"
          },
          {
            "expr": "sum(rate(http_requests_total{service=\"brain\",namespace=\"huskyapply\",status=~\"5.*\"}[5m])) / sum(rate(http_requests_total{service=\"brain\",namespace=\"huskyapply\"}[5m])) * 100",
            "legendFormat": "Brain Error Rate %"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "max": 10,
            "unit": "percent"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        }
      },
      {
        "id": 11,
        "title": "Node Resource Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "1 - rate(node_cpu_seconds_total{mode=\"idle\"}[5m])",
            "legendFormat": "{{instance}} CPU Usage"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "max": 1,
            "unit": "percentunit"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 40
        }
      },
      {
        "id": 12,
        "title": "Scaling Events",
        "type": "table",
        "targets": [
          {
            "expr": "increase(kube_hpa_status_current_replicas{namespace=\"huskyapply\"}[5m])",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 40
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "namespace",
          "type": "constant",
          "current": {
            "value": "huskyapply"
          }
        },
        {
          "name": "interval",
          "type": "interval",
          "options": [
            "30s",
            "1m",
            "5m",
            "10m",
            "30m",
            "1h"
          ],
          "current": {
            "value": "5m"
          }
        }
      ]
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "30s"
  }
}